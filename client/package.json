{
  "name": "client",
  "version": "0.1.0",
  "description": "Apollo Client",
  "main": "main.tsx",
  "typings": "types.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "prebuild": "tsc --noEmit",
    "download-schema": "apollo service:download --endpoint=http://localhost:4000/ graphql-schema.json",
    "generate": "graphql-codegen --config config/codegen.js",
    "codegen": "npm run download-schema && npm run generate",
    "predev": "npm run generate"
  },
  "dependencies": {
    "@apollo/client": "^3.4.15",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@graphql-codegen/cli": "^2.3.1",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.3",
    "@reach/dialog": "^0.16.2",
    "@reach/visually-hidden": "^0.16.0",
    "@react-spring/web": "^9.3.2",
    "graphql": "^15.6.0",
    "graphql-anywhere": "^4.2.7",
    "graphql-tag": "^2.12.6",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-feather": "^2.0.9",
    "react-map-gl": "^6.1.18",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^16.10.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "apollo": "^2.33.6",
    "typescript": "^4.3.2",
    "vite": "^2.5.4"
  }
}
