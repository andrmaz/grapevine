{
  "name": "server",
  "version": "0.1.0",
  "description": "Apollo Server",
  "main": "index.ts",
  "typings": "graphql.ts",
  "scripts": {
    "test": "jest --bail",
    "test:ci": "jest --ci --bail --silent",
    "test:watch": "jest --watch --bail",
    "start": "ts-node src/index.ts",
    "dev": "nodemon src/index.ts",
    "generate": "graphql-codegen --config codegen.yml",
    "prestart": "npm run generate",
    "tsc": "tsc -b --verbose",
    "build": "node build/src/index.js",
    "prebuild": "npm run tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-codegen/typescript-mongodb": "^2.3.0",
    "@graphql-tools/schema": "^8.3.5",
    "@graphql-tools/utils": "^8.6.2",
    "apollo-datasource-mongodb": "^0.5.2",
    "apollo-datasource-rest": "^3.3.0",
    "apollo-server": "^3.4.1",
    "apollo-server-core": "^3.11.0",
    "apollo-server-express": "^3.6.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "graphql": "^15.8.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.6.4",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^3.1.2",
    "mongodb": "^4.3.1",
    "mongoose": "^6.11.3",
    "ws": "^8.5.0"
