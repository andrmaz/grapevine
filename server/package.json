    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-document-nodes": "2.1.4",
    "@graphql-codegen/typescript-resolvers": "2.2.1",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.15",
    "jest": "^28.1.0",
    "nodemon": "^2.0.20",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
